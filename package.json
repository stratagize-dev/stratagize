{
  "name": "stragize",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test:e2e": "playwright test",
    "generate:strava-client": "/usr/local/Cellar/swagger-codegen@2/2.4.31/bin/swagger-codegen generate --additional-properties modelPropertyNaming=original -i https://developers.strava.com/swagger/swagger.json -l typescript-fetch -o src/shared/strava-client",
    "db:local:generate:types": "sudo Nangaparbat1supabase gen types typescript --local  > database.types.ts",

    "db:local:start": "sudo supabase start",
    "db:local:stop": "sudo supabase stop",
    "db:local:migrate": "sudo supabase db reset",
    "ngrok": "ngrok http 3001 --domain valid-factual-barnacle.ngrok-free.app",
    "inngest:dev": "npx inngest-cli@latest dev"
  },
  "dependencies": {
    "@baselime/node-opentelemetry": "0.3.0",
    "@headlessui/react": "1.7.17",
    "@heroicons/react": "2.1.1",
    "@supabase/supabase-js": "2.39.1",
    "autoprefixer": "10.4.16",
    "date-fns": "3.6.0",
    "inngest": "^3.22.2",
    "jotai": "2.6.0",
    "jsonwebtoken": "9.0.2",
    "next": "14.0.4",
    "next-auth": "4.24.5",
    "ngrok": "5.0.0-beta.2",
    "portable-fetch": "^3.0.0",
    "postcss": "8.4.38",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.1",
    "tailwindcss": "3.4.3",
    "typescript": "5.3.3"
  },
  "devDependencies": {
    "@inngest/eslint-plugin": "^0.0.7",
    "@playwright/test": "1.40.1",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.1.2",
    "@types/cookie": "0.6.0",
    "@types/jsonwebtoken": "9.0.5",
    "@types/node": "20.10.5",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "prettier": "3.1.1",
    "supabase": "1.187.10"
  }
